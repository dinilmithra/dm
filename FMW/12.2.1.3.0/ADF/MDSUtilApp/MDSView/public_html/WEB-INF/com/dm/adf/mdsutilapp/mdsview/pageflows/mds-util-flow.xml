<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="mds-util-flow">
    <default-activity>mds-util-fragment</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>MDSUtilBean</managed-bean-name>
      <managed-bean-class>com.dm.adf.mdsutilapp.mdsview.beans.MDSUtilBean</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="mds-util-fragment">
      <page>/com/dm/adf/mdsutilapp/mdsview/fragments/mds-util-fragment.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
