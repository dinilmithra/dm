      <af:dialog id="d16" type="okCancel" title="#{wfBundle.APPROVE_TITLE}"
                 contentWidth="340" closeIconVisible="false"
                 dialogListener="#{wccDocInfoBean.workflowApproveDialogListener}"
                 affirmativeTextAndAccessKey="#{wfBundle.APPROVE}">
        <af:panelGroupLayout id="pgl61" layout="vertical">
          <af:outputFormatted value="#{af:formatString2(wfBundle.APPROVE_INSTR,
                              wccDocInfoBean.revision.displayName,
                              wccDocInfoBean.revision.workflow.currentStep.DWfStepName)}"
                              id="of8" styleClass="WccTextLightBreakLong"/>
          <af:spacer id="s54" height="10"/>
        </af:panelGroupLayout>
      </af:dialog>
    </af:popup>
    <af:popup childCreation="deferred" autoCancel="disabled"
              id="popupRejectDocument" contentDelivery="lazyUncached"
              resetEditableValues="whenCanceled">
      <af:dialog id="d17" type="none" title="#{wfBundle.REJECT_TITLE}"
                 contentWidth="340">
        <f:facet name="buttonBar">
          <af:panelGroupLayout layout="horizontal" id="pgl60" halign="end"
                               valign="bottom" styleClass="AFStretchWidth">
            <af:button text="#{wfBundle.REJECT}" id="cb16"
                              actionListener="#{wccDocInfoBean.rejectDocumentActionListener}"
                              partialSubmit="true">
              <f:attribute name="popupId" value="popupRejectDocument"/>
            </af:button>
            <af:button text="#{libBundle.CANCEL_BUTTON}" id="cb17"
                              partialSubmit="true" immediate="true"
                              actionListener="#{wccDocInfoBean.closePopup}">
              <f:attribute name="popupId" value="popupRejectDocument"/>
              <af:resetListener type="action"/>
            </af:button>
          </af:panelGroupLayout>
        </f:facet>
        <af:panelGroupLayout id="pgl62" layout="vertical">
          <af:outputFormatted value="#{af:formatString2(wfBundle.REJECT_INSTR,
                              wccDocInfoBean.revision.displayName,
                              wccDocInfoBean.revision.workflow.currentStep.DWfStepName)}"
                              id="of9" styleClass="WccTextLightBreakLong"/>
          <af:spacer height="10" id="s55"/>
          <af:panelFormLayout id="pfl6" labelAlignment="top">
            <af:inputText label="#{wfBundle.REJECT_REASON}" id="it4" rows="4"
                          contentStyle="width:325.0px;resize:none;"
                          value="#{wccDocInfoBean.workflowRejectReason}"/>
          </af:panelFormLayout>
        </af:panelGroupLayout>
      </af:dialog>
    </af:popup>
    <af:popup childCreation="deferred" autoCancel="disabled" id="popupWfApproveDoc"
              contentDelivery="lazyUncached" clientComponent="true" animate="false"
              popupCanceledListener="#{wccWorkflowApprovalBean.popupCancelListener}"
              popupFetchListener="#{wccWorkflowApprovalBean.popupFetchListener}">
      <af:dialog id="d10" stretchChildren="first"
                 clientComponent="true" contentWidth="500"
                 contentHeight="#{fn:length(wccWorkflowApprovalBean.metadataForm.items) > 0 ? 250 : 160}"
                 title="#{wfBundle.SIGN_AND_APPROVE_TITLE}" type="none"
                 helpTopicId="#{wccel:getHelpTopicId('DOCUMENT_SIGN_AND_APPROVE')}">
        <f:facet name="buttonBar">
          <af:panelGroupLayout id="pgl1" halign="end" layout="horizontal"
                               styleClass="AFStretchWidth">
            <af:button text="#{wfBundle.SIGN_AND_APPROVE}" id="cb9"
                              actionListener="#{wccWorkflowApprovalBean.approveActionListener}"
                              partialSubmit="true">
              <f:attribute name="popupId" value="popupWfApproveDoc"/>
            </af:button>
            <af:button text="#{libBundle.CANCEL_BUTTON}" id="cb10" 
                              actionListener="#{wccWorkflowApprovalBean.cancelActionListener}"
                              partialSubmit="true" immediate="true">
              <f:attribute name="popupId" value="popupWfApproveDoc"/>
            </af:button>
          </af:panelGroupLayout>
        </f:facet>
        <ui:include src="/wcc/approve.jsff"/>
      </af:dialog>
    </af:popup>
    
    <af:resource type="javascript">
      function checkInUploadPopupClosed(event)
      {
        AdfCustomEvent.queue(event.getSource(), "checkInUploadPopupClosed", 
        {
        },
true);
      }
    </af:resource>
    <c:if test="#{wccel:isCheckoutAndOpenInNativeEnabled()}">
      <af:resource type="javascript" source="#{wccel:getCoaoJavaScript()}"/>
    </c:if>
  </af:group>
</ui:composition>
