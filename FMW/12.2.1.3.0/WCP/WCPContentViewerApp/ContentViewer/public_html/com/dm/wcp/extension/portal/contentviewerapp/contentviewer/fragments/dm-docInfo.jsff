<ui:composition xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
                xmlns:wccel="http://xmlns.oracle.com/wcc/adf/el"
                xmlns:c="http://java.sun.com/jsp/jstl/core"
                xmlns:fn="http://java.sun.com/jsp/jstl/functions"
                xmlns:vwr="http://xmlns.oracle.com/imaging/viewer">
  <c:set var="diBundle"
         value="#{adfBundle['oracle.wcc.adf.vc.resource.DocInfoBundle']}"/>
  <c:set var="docBundle"
         value="#{adfBundle['oracle.wcc.adf.vc.resource.DocumentsBundle']}"/>
  <c:set var="mainBundle"
         value="#{adfBundle['oracle.wcc.adf.vc.resource.MainBundle']}"/>
  <c:set var="viewcontrollerBundle"
         value="#{adfBundle['oracle.wcc.adf.vc.resource.UploadBundle']}"/>
  <c:set var="libBundle"
         value="#{adfBundle['oracle.wcc.adf.vc.resource.LibrariesBundle']}"/>
  <c:set var="wfBundle"
         value="#{adfBundle['oracle.wcc.adf.vc.resource.WorkflowBundle']}"/>
  <c:set var="pb"
         value="#{adfBundle['oracle.wcc.adf.vc.resource.PreferencesBundle']}"/>
  <c:set var="compactMode"
         value="#{not empty pageFlowScope.taskFlowContext and not empty pageFlowScope.compactMode and pageFlowScope.compactMode}"/>
  <c:set var="inplaceNavigation"
         value="#{(not empty pageFlowScope.taskFlowContext) and pageFlowScope.inplaceNavigation}"/>
  <af:resource type="javascript" source="#{wccel:getVersionedResource('/resources/wcc/js/wcc.js')}" />
  <af:resource type="javascript" source="#{wccel:getVersionedResource('/resources/wcc/js/wcchelp.js')}" />
  <af:resource type="javascript">
    //<![CDATA[
      var ___docPropsIsInAccssMode =  "#{(requestContext.accessibilityMode eq 'SCREEN_READER') ? 'true' : 'false'}";
      oracle.wcc.adf.docProperties.hideDocRevHints();
    //]]>
  </af:resource>
  <af:group id="gDocInfo">
    <af:panelStretchLayout id="psl1" topHeight="auto"
                           styleClass="AFStretchWidth WccDocPropsPage #{empty pageFlowScope.taskFlowContext ? '' : ' WccTaskflow'}#{compactMode ? ' WccCompactMode' : ''}"
                           inlineStyle="#{(not empty pageFlowScope.taskFlowContext) ? 'min-height:600px;' : ''}"
                           dimensionsFrom="parent">
      <f:facet name="top">
      
        <af:panelGroupLayout id="pgl16" layout="vertical" styleClass="WccDpHeader">
          
          <af:panelBorderLayout id="pbl1" styleClass="WccDpBrandingBar" rendered="#{empty pageFlowScope.taskFlowContext}">
            <f:facet name="start">
              <c:if test="#{empty pageFlowScope.taskFlowContext}">
                <af:panelGroupLayout id="pgl3" layout="horizontal" valign="bottom" halign="start">
                  <af:panelGroupLayout id="pgl5" layout="horizontal">
                    <af:commandImageLink id="cil1" styleClass="WccBrandingBarLogo"
                                         shortDesc="#{mainBundle.BRANDING_LOGO_LINK_DESCRIPTION}"
                                         partialSubmit="true" action="wccmain"
                                         rendered="#{wccel:getCustomBrandingLogo() == null}"/>
                    <af:commandImageLink id="cil2"
                                         icon="#{wccel:getCustomBrandingLogo()}"
                                         shortDesc="#{mainBundle.BRANDING_LOGO_LINK_DESCRIPTION}"
                                         partialSubmit="true" action="wccmain"
                                         rendered="#{wccel:getCustomBrandingLogo() != null}"/>
                    <af:outputText id="ot1" styleClass="WccBrandingBarTitle"
                                   value="#{wccel:getCustomBrandingTitle() == null
                                          ? mainBundle.WEBCENTERCONTENT : wccel:getCustomBrandingTitle()}"/>
                  </af:panelGroupLayout>
                </af:panelGroupLayout>
              </c:if>
            </f:facet>
            
            <f:facet name="end">
              <af:panelGroupLayout id="pglEn1" halign="end" layout="horizontal"
                                   inlineStyle="padding-top:2px;">
                <af:menuBar id="mbDpUsr" clientComponent="true" visible="true"
                            rendered="#{empty pageFlowScope.taskFlowContext}">
                  <af:menu text="#{wccSessionData.user.displayName}" id="mnDp1"
                           clientComponent="true">
                    <af:group id="gmDp1">
                      <af:commandMenuItem text="#{mainBundle.ABOUT}"
                                          id="cmiDp1">
                        <af:showPopupBehavior popupId="aboutPopup"
                                              triggerType="action"
                                              align="afterEnd"
                                              alignId="mbDpUsr"/>
                      </af:commandMenuItem>
                    </af:group>
                    <af:group id="g1">
                      <af:menu text="#{mainBundle.HELP}" id="mHelp">
                        <af:commandMenuItem text="#{mainBundle.DOCUMENT_LIBRARY}" id="cmi5">
                          <af:clientAttribute name="helpTopicUrl" value="#{wccel:getDocumentLibraryUrl()}"/>
                          <af:clientListener method="oracle.wcc.adf.help.launchHelp" type="action"/>
                        </af:commandMenuItem>
                        <af:commandMenuItem text="#{mainBundle.HELP_CONTENTS}" id="cmi8">
                          <af:clientAttribute name="helpTopicUrl" value="#{wccel:getHelpContentsUrl()}"/>
                          <af:clientListener method="oracle.wcc.adf.help.launchHelp" type="action"/>
                        </af:commandMenuItem>
                        <af:commandMenuItem text="#{mainBundle.HELP_FOR_THIS_PAGE}" id="cmi4">
                          <af:clientAttribute name="helpTopicUrl" value="#{wccel:getHelpTopicUrl('DOCUMENT_PROPERTIES_PAGE')}"/>
                          <af:clientListener method="oracle.wcc.adf.help.launchHelp" type="action"/>
                        </af:commandMenuItem>
                        <af:commandMenuItem text="#{mainBundle.HOW_DO_I}" id="cmi6">
                          <af:clientAttribute name="helpTopicUrl" value="#{wccel:getHdiTopicUrl('DOCUMENT_PROPERTIES_PAGE')}"/>
                          <af:clientListener method="oracle.wcc.adf.help.launchHelp" type="action"/>
                        </af:commandMenuItem>
                      </af:menu>
                    </af:group>
                    <af:group id="gmDp2">
                      <af:goMenuItem text="#{mainBundle.LOGOUT}" id="gmiDp1"
                                     destination="/adfAuthentication?logout=true&amp;end_url=/faces/wccmain"/>
                    </af:group>
                  </af:menu>
                </af:menuBar>
                <af:popup childCreation="deferred" autoCancel="enabled"
                          id="aboutPopup" animate="false"
                          contentDelivery="lazy">
                  <af:panelWindow id="pw1" title="#{mainBundle.ABOUT}"
                                  modal="false" closeIconVisible="true"
                                  contentWidth="410">
                    <ui:include src="/wcc/about.jsff"/>
                  </af:panelWindow>
                </af:popup>
              </af:panelGroupLayout>
            </f:facet>
          </af:panelBorderLayout>
          
            <af:panelGroupLayout id="pgl13" styleClass="WccDpBreadcrumbBar"
                                 rendered="#{empty pageFlowScope.taskFlowContext}">
              <c:if test="${wccDocInfoBean.revision != null and not wccDocInfoBean.isFiled}">
                <af:outputText value="#{wccel:getCustomBrandingTitle() == null
                                        ? diBundle.ORACLE_WEBCENTER_CONTENT
                                        : wccel:getLabelWithColon(wccel:getCustomBrandingTitle())}"
                               id="s444" styleClass="WccFolderPathText"
                               rendered="#{empty pageFlowScope.taskFlowContext}"/>
                <af:spacer width="5" id="pg1345"/>
                <af:outputText value="#{diBundle.UNFILED_DOCUMENT}" id="pg153"
                               styleClass="WccFolderPathText"/>
                <af:spacer width="15" id="pg154"/>
                <af:goLink text="#{diBundle.ADD_TO_LIBRARY}" id="gl21"
                           shortDesc="#{diBundle.ADD_TO_LIBRARY}"
                           styleClass="WccFolderPathLinkText"
                           rendered="#{wccDocInfoBean.capabilities['FILE_DOCUMENT'] and not wccDocInfoBean.workflowInitialContribution}"
                           clientComponent="true">
                  <af:clientAttribute name="instanceKey" value="#{wccInstanceBean.instanceKey}"/>
                  <af:clientAttribute name="clickMethod" value="docProperties.handleUnsavedChanges"/>
                  <af:clientListener type="click" method="oracle.wcc.adf.instances.callInstanceHandler"/>
                  <af:serverListener method="#{wccDocInfoBean.showAddToLibraryPopup}"
                                     type="showAddToLibraryPopup"/>
                  <af:clientAttribute name="serverListener"
                                      value="showAddToLibraryPopup"/>
                </af:goLink>
              </c:if>
              <c:if test="${wccDocInfoBean.revision != null and wccDocInfoBean.isFiled}">
                <af:outputText value="#{wccel:getCustomBrandingTitle() == null
                                        ? diBundle.ORACLE_WEBCENTER_CONTENT
                                        : wccel:getLabelWithColon(wccel:getCustomBrandingTitle())}"
                               id="ot4" styleClass="WccFolderPathText"
                               rendered="#{empty pageFlowScope.taskFlowContext}"/>
                <af:spacer width="5" id="s9"/>
                <af:switcher facetName="#{wccDocInfoBean.parsedPath.pathType}">
                  <f:facet name="ENTERPRISE_LIBRARY">
                    <af:outputText value="#{mainBundle.ENTERPRISE_LIBRARIES}"
                                   id="otEL" styleClass="WccFolderPathText"/>
                  </f:facet>
                  <f:facet name="SYSTEM_LIBRARY">
                    <af:outputText value="#{mainBundle.SYSTEM_LIBRARIES}"
                                   id="otSL" styleClass="WccFolderPathText"/>
                  </f:facet>
                </af:switcher>
                <af:iterator value="#{wccDocInfoBean.parsedPath.breadcrumbs}"
                             var="pathToken" id="i12" rows="0">
                  <af:outputText value="#{pathToken.delimiter}" id="otde"
                                 styleClass="WccFolderPathText"
                                 rendered="#{not pathToken.lastItem}"/>
                  <af:goLink id="l1" text="#{wccel:truncateAt(pathToken.itemName, 18)}"
                             styleClass="WccFolderPathLinkText"
                             shortDesc="#{pathToken.itemName}"
                             rendered="#{not pathToken.lastItem}"
                             destination="#{wccel:getFolderUrlByPath(pathToken.itemPath)}"
                             clientComponent="true">
                    <af:clientAttribute name="instanceKey" value="#{wccInstanceBean.instanceKey}"/>
                    <af:clientAttribute name="clickMethod" value="docProperties.handleUnsavedChanges"/>
                    <af:clientListener type="click" method="oracle.wcc.adf.instances.callInstanceHandler"/>
                  </af:goLink>
                </af:iterator>
              </c:if>
            </af:panelGroupLayout>

          <af:panelGroupLayout id="pgl20" layout="horizontal"
                               valign="middle"
                               inlineStyle="padding:0 12px;"
                               rendered="#{inplaceNavigation}">
            <af:link id="l2" partialSubmit="true"
                     icon="#{adfFacesContext.rightToLeft ? '/resources/wcc/images/back_caret_rtl_act.png' : '/resources/wcc/images/back_caret_act.png'}"
                     disabledIcon="#{adfFacesContext.rightToLeft ? '/resources/wcc/images/back_caret_rtl_dis.png' : '/resources/wcc/images/back_caret_dis.png'}"
                     hoverIcon="#{adfFacesContext.rightToLeft ? '/resources/wcc/images/back_caret_rtl_ovr.png' : '/resources/wcc/images/back_caret_ovr.png'}"
                     text="#{diBundle.BACK_LINK}"
                     action="#{wccDocInfoBean.getMainViewOutcome}">
              <af:clientAttribute name="backLink" value="#{true}"/>
              <af:clientAttribute name="instanceKey" value="#{wccInstanceBean.instanceKey}"/>
              <af:clientAttribute name="actionMethod" value="docProperties.handleUnsavedChanges"/>
              <af:clientListener type="action" method="oracle.wcc.adf.instances.callInstanceHandler"/>
            </af:link>
          </af:panelGroupLayout>
          
          <c:if test="#{wccDocInfoBean.revision != null}">
          <af:panelBorderLayout id="pbl2" styleClass="WccDpTitleBar">
            <af:panelGroupLayout id="pgl15" layout="default"
                                 styleClass="WccDpTitleGroup" valign="#{empty pageFlowScope.taskFlowContext ? 'middle' : 'middle'}"
                                 inlineStyle="display:inline-block;">
              <af:outputText value="#{wccel:truncateAt(wccDocInfoBean.revisionDisplayName, 37)}"
                             id="cil0" styleClass="WccTitleGroupNameText"
                             shortDesc="#{fn:length(wccDocInfoBean.revisionDisplayName) gt 37 ? wccDocInfoBean.revisionDisplayName : ' '}"/>
              <af:outputText value="#{wccDocInfoBean.revision.DDocTitle}"
                             id="ot13" styleClass="WccDocTitleText"
                             rendered="#{wccDocInfoBean.showTitle and not compactMode}"/>
              <af:commandImageLink id="cil3"
                                   actionListener="#{wccDocInfoBean.setToggleFavourite}"
                                   icon="#{wccDocInfoBean.toggleFavourite ?
                                           '/resources/wcc/images/favorite_on_ena.png' : '/resources/wcc/images/favorite_off_ena.png'}"
                                   hoverIcon="#{wccDocInfoBean.toggleFavourite ?
                                                '/resources/wcc/images/favorite_on_ovr.png' : '/resources/wcc/images/favorite_off_ovr.png'}"
                                   partialSubmit="true"
                                   partialTriggers="cil3 fav"
                                   shortDesc="#{wccDocInfoBean.revision.DStatus eq 'EXPIRED'
                                                          ? ''
                                                          : (wccDocInfoBean.toggleFavourite ?
                                                            diBundle.DESC_UNFAVORITE : docBundle.MARK_AS_FAVORITE)}"
                                   disabled="#{wccDocInfoBean.revision.DStatus eq 'EXPIRED'}"
                                   rendered="#{securityContext.authenticated}"/>
              <af:commandImageLink icon="/resources/wcc/images/link_ena.png"
                                   id="cil6"
                                   shortDesc="#{diBundle.ACTION_GET_LINKS}"
                                   disabledIcon="/resources/wcc/images/link_dis.png"
                                   hoverIcon="/resources/wcc/images/link_ovr.png"
                                   depressedIcon="/resources/wcc/images/link_dwn.png">
                <af:clientAttribute name="dID"
                                    value="#{wccDocInfoBean.revision.DID}"/>
                <af:clientAttribute name="dDocName"
                                    value="#{wccDocInfoBean.revision.DDocName}"/>
                <af:showPopupBehavior popupId="docPaths"/>
              </af:commandImageLink>
              <af:commandImageLink icon="/resources/wcc/images/refresh_ena.png"
                                   id="clDpRef1" partialSubmit="true"
                                   shortDesc="#{docBundle.REFRESH}"
                                   disabledIcon="/resources/wcc/images/refresh_dis.png"
                                   hoverIcon="/resources/wcc/images/refresh_ovr.png"
                                   depressedIcon="/resources/wcc/images/refresh_dwn.png">
                <af:serverListener method="#{wccDocInfoBean.doRefreshDocPropertiesPage}"
                                   type="doRefreshDocPropertiesPage"/>
                <af:clientAttribute name="instanceKey" value="#{wccInstanceBean.instanceKey}"/>
                <af:clientAttribute name="actionMethod" value="docProperties.handleUnsavedChanges"/>
                <af:clientListener type="action" method="oracle.wcc.adf.instances.callInstanceHandler"/>
                <af:clientAttribute name="serverListener"
                                    value="doRefreshDocPropertiesPage"/>
              </af:commandImageLink>
            </af:panelGroupLayout>
            <f:facet name="end">
              <af:panelGroupLayout id="pgl9" styleClass="WccDpToolbar"
                                   rendered="#{wccDocInfoBean.revision != null}"
                                   partialTriggers="chkout cancelchkout"
                                   layout="horizontal">
                <c:if test="${wccDocInfoBean.checkedOut}">
                  <c:if test="${not wccDocInfoBean.contentCheckedOutByCurrentUser}">
                    <af:image id="ctb12"
                              source="/resources/wcc/images/checked_out_by_other.png"
                              shortDesc="#{af:formatString(diBundle.CHECKED_OUT_BY_TOOLTIP,
                                            wccel:getDisplayNameForUser(wccDocInfoBean.checkOutByUserName))}"/>
                  </c:if>
                  <c:if test="${wccDocInfoBean.contentCheckedOutByCurrentUser}">
                    <af:image id="ctb121"
                              source="/resources/wcc/images/checked_out_by_me.png"
                              shortDesc="#{diBundle.CHECKED_OUT_BY_CURRENT_USER_TOOLTIP}"/>
                  </c:if>
                  <af:spacer width="20" height="10" id="s10"/>
                </c:if>
                  <af:button text="#{diBundle.BUTTON_CHECK_IN}"
                                           id="ciBtnLh" clientComponent="true"
                                           icon="/resources/wcc/images/upload_ena.png"
                                           hoverIcon="/resources/wcc/images/upload_ovr.png"
                                           depressedIcon="/resources/wcc/images/upload_dwn.png"
                                           disabledIcon="/resources/wcc/images/upload_dis.png"
                                           partialSubmit="true"
                                           shortDesc="#{diBundle.BUTTON_CHECK_IN}"
                                           disabled="#{not wccDocInfoBean.capabilities['CHECK_IN']}"
                             rendered="#{securityContext.authenticated}">
                    <af:setActionListener from="#{wccDocInfoBean.revision}"
                                          to="#{wccCheckInUploadBean.revision}"/>
                    <af:setActionListener from=""
                                          to="#{wccCheckInUploadBean.docProfile}"/>
                    <af:setActionListener from="CheckInNewRevision"
                                          to="#{wccCheckInUploadBean.uploadType}"/>
                    <af:setActionListener from="true"
                                          to="#{wccCheckInUploadBean.docPropContext}"/>
                    <af:clientAttribute name="docPropContext" value="true"/>
                    <af:clientAttribute name="uploadType"
                                        value="CheckInNewRevision"/>
                    <af:clientAttribute name="instanceKey" value="#{wccInstanceBean.instanceKey}"/>
                    <af:clientAttribute name="actionMethod" value="docProperties.handleUnsavedChanges"/>
                    <af:clientListener type="action" method="oracle.wcc.adf.instances.callInstanceHandler"/>
                    <af:clientAttribute name="serverListener"
                                        value="showCheckinPopupFromBtn"/>
                    <af:serverListener method="#{wccDocInfoBean.launchCheckinDialog}"
                                       type="showCheckinPopupFromBtn"/>
                  </af:button>
                <af:menuBar id="mb1"
                            rendered="#{securityContext.authenticated}">
                  <af:menu text="#{diBundle.MORE_ACTIONS}" id="m2"
                           shortDesc="#{diBundle.MORE_ACTIONS}"
                           styleClass="WccMenuItemButton" inlineStyle="padding-left:3px; padding-right:3px;">
                    <af:group id="g3"
                              rendered="#{wccDocInfoBean.revision.inWorkflow}">
                      <af:commandMenuItem id="cmi3" partialSubmit="true"
                                          text="#{wfBundle.APPROVE}"
                                          shortDesc="#{wfBundle.APPROVE}"
                                          rendered="#{wccDocInfoBean.capabilities['APPROVE']
                                                      or wccDocInfoBean.capabilities['SIGN_AND_APPROVE']}"
                                          actionListener="#{wccDocInfoBean.approveDocActionListener}"/>
                      <af:commandMenuItem id="cmi2" partialSubmit="true"
                                          text="#{wfBundle.REJECT}"
                                          rendered="#{wccDocInfoBean.capabilities['REJECT']}"
                                          shortDesc="#{wfBundle.REJECT}">
                        <af:showPopupBehavior popupId="popupRejectDocument"/>
                      </af:commandMenuItem>
                    </af:group>
                    <af:group id="gp1">
                      <af:commandMenuItem text="#{wccDocInfoBean.workflowCheckInEdit or wccDocInfoBean.workflowInitialContribution ? diBundle.ACTION_WORKFLOW_CHECK_IN_EDIT : diBundle.ACTION_CHECK_IN_NEW_REVISION}"
                                          shortDesc="#{wccDocInfoBean.workflowCheckInEdit or wccDocInfoBean.workflowInitialContribution ? diBundle.ACTION_WORKFLOW_CHECK_IN_EDIT : diBundle.ACTION_CHECK_IN_NEW_REVISION}"
                                          id="ciMnuRh" partialSubmit="true"
                                          disabled="#{!wccDocInfoBean.capabilities['CHECK_IN']}">
                        <af:setActionListener from="#{wccDocInfoBean.revision}"
                                              to="#{wccCheckInUploadBean.revision}"/>
                        <af:setActionListener from=""
                                              to="#{wccCheckInUploadBean.docProfile}"/>
                        <af:setActionListener from="CheckInNewRevision"
                                              to="#{wccCheckInUploadBean.uploadType}"/>
                        <af:setActionListener from="true"
                                              to="#{wccCheckInUploadBean.docPropContext}"/>
                        <af:clientAttribute name="docPropContext" value="true"/>
                        <af:clientAttribute name="uploadType"
                                            value="CheckInNewRevision"/>
                        <af:clientAttribute name="instanceKey" value="#{wccInstanceBean.instanceKey}"/>
                        <af:clientAttribute name="actionMethod" value="docProperties.handleUnsavedChanges"/>
                        <af:clientListener type="action" method="oracle.wcc.adf.instances.callInstanceHandler"/>
                        <af:clientAttribute name="serverListener"
                                            value="showCheckinPopupFromMenu"/>
                        <af:serverListener method="#{wccDocInfoBean.launchCheckinDialog}"
                                           type="showCheckinPopupFromMenu"/>
                      </af:commandMenuItem>
                      <af:commandMenuItem text="#{diBundle.ACTION_CHECK_OUT}"
                                          shortDesc="#{diBundle.ACTION_CHECK_OUT}"
                                          id="chkout"
                                          disabled="#{!wccDocInfoBean.capabilities['CHECK_OUT']}"
                                          rendered="#{!wccDocInfoBean.capabilities['CANCEL_CHECK_OUT']}"
                                          partialTriggers="chkout cancelchkout DOCTAB:editbtn"
                                          partialSubmit="true">
                        <af:serverListener method="#{wccDocInfoBean.doCheckout}"
                                           type="doCheckout"/>
                        <af:clientAttribute name="instanceKey" value="#{wccInstanceBean.instanceKey}"/>
                        <af:clientAttribute name="actionMethod" value="docProperties.handleUnsavedChanges"/>
                        <af:clientListener type="action" method="oracle.wcc.adf.instances.callInstanceHandler"/>
                        <af:clientAttribute name="serverListener"
                                            value="doCheckout"/>
                      </af:commandMenuItem>
                      <af:commandMenuItem text="#{diBundle.ACTION_CANCEL_CHECK_OUT}"
                                          shortDesc="#{diBundle.ACTION_CANCEL_CHECK_OUT}"
                                          id="cancelchkout"
                                          rendered="#{wccDocInfoBean.capabilities['CANCEL_CHECK_OUT']}"
                                          partialTriggers="chkout cancelchkout DOCTAB:editbtn"
                                          partialSubmit="true">
                        <af:serverListener method="#{wccDocInfoBean.undoCheckout}"
                                           type="undoCheckout"/>
                        <af:clientAttribute name="instanceKey" value="#{wccInstanceBean.instanceKey}"/>
                        <af:clientAttribute name="actionMethod" value="docProperties.handleUnsavedChanges"/>
                        <af:clientListener type="action" method="oracle.wcc.adf.instances.callInstanceHandler"/>
                        <af:clientAttribute name="serverListener"
                                            value="undoCheckout"/>
                      </af:commandMenuItem>
                      <af:commandMenuItem text="#{diBundle.ACTION_CHECK_OUT_EDIT}"
                                          id="chkoutedit"
                                          shortDesc="#{diBundle.ACTION_CHECK_OUT_EDIT}"
                                          disabled="#{!wccDocInfoBean.capabilities['CHECK_OUT']}"
                                          rendered="#{wccDocInfoBean.coaoValidExtn 
                                                      and not wccDocInfoBean.workflowInitialContribution
                                                      and wccel:isAppletSupportedClient()}"
                                          partialTriggers="chkout cancelchkout"
                                          partialSubmit="true">
                        <f:attribute value="#{wccDocInfoBean.revision}"
                                     name="currentRevision"/>
                        <af:clientAttribute value="#{wccDocInfoBean.revision.DDocName}"
                                            name="dDocName"/>
                        <af:clientAttribute value="#{wccDocInfoBean.revision.DDocTitle}"
                                            name="dDocTitle"/>
                        <af:clientAttribute value="#{wccDocInfoBean.revision.DExtension}"
                                            name="dExtension"/>
                        <af:clientAttribute value="#{wccDocInfoBean.revision.DOriginalName}"
                                            name="dOriginalName"/>
                        <af:clientAttribute name="instanceKey" value="#{wccInstanceBean.instanceKey}"/>
                        <af:clientAttribute name="actionMethod" value="docProperties.handleCheckOutAndOpen"/>
                        <af:clientListener type="action" method="oracle.wcc.adf.instances.callInstanceHandler"/>
                        <af:serverListener method="#{wccDocInfoBean.checkOutAndEditCallback}"
                                           type="coaoCallback"/>     
                      </af:commandMenuItem>
                    </af:group>
                    <af:group id="gp3">
                      <af:commandMenuItem text="#{diBundle.ACTION_GET_LINKS}"
                                          id="getlinks"
                                          shortDesc="#{diBundle.ACTION_GET_LINKS}"
                                          clientComponent="true">
                        <af:clientAttribute name="dID"
                                            value="#{wccDocInfoBean.revision.DID}"/>
                        <af:clientAttribute name="dDocName"
                                            value="#{wccDocInfoBean.revision.DDocName}"/>
                        <af:showPopupBehavior popupId="docPaths"/>
                      </af:commandMenuItem>
                      <af:commandMenuItem text="#{diBundle.ACTION_FAVORITE}
                                                    #{((requestContext.accessibilityMode eq 'SCREEN_READER') and
                                                    (wccDocInfoBean.toggleFavourite)) ? docBundle.SELECTED_OPTION : ''}"
                                          id="fav"
                                          icon="#{wccDocInfoBean.toggleFavourite ?
                                                 '/resources/wcc/images/menu_check.png': ''}"
                                          partialTriggers="fav cil3"
                                          shortDesc="#{wccDocInfoBean.revision.DStatus eq 'EXPIRED'
                                                       ? ''
                                                       : (wccDocInfoBean.toggleFavourite ?
                                                         diBundle.DESC_UNFAVORITE : docBundle.MARK_AS_FAVORITE)}"
                                          partialSubmit="true"
                                          disabled="#{wccDocInfoBean.revision.DStatus eq 'EXPIRED'}"
                                          actionListener="#{wccDocInfoBean.setToggleFavourite}"></af:commandMenuItem>
                      <af:commandMenuItem text="#{diBundle.ACTION_FOLLOW}"
                                          id="follow"
                                          disabled="#{!wccDocInfoBean.capabilities['FOLLOW']}"
                                          rendered="#{!wccDocInfoBean.capabilities['UNFOLLOW']}"
                                          shortDesc="#{diBundle.ACTION_FOLLOW}"
                                          partialTriggers="follow unfollow">
                        <af:serverListener method="#{wccDocInfoBean.followDocument}"
                                           type="followDocument"/>
                        <af:clientAttribute name="instanceKey" value="#{wccInstanceBean.instanceKey}"/>
                        <af:clientAttribute name="actionMethod" value="docProperties.handleUnsavedChanges"/>
                        <af:clientListener type="action" method="oracle.wcc.adf.instances.callInstanceHandler"/>
                        <af:clientAttribute name="serverListener"
                                            value="followDocument"/>
                      </af:commandMenuItem>
                      <af:commandMenuItem text="#{diBundle.ACTION_FOLLOW}
